<slag>
    <channel id="bd" file="../../samples/bd.wav" active="1" />
    <channel id="snare" file="../../samples/snare.wav" active="1" />
    <channel id="ch" file="../../samples/ch.wav" active="1" />
    <channel id="ride" file="../../samples/ride.wav" active="1" />
    <channel id="bass" file="../../samples/Novation_BassStation_1.wav" active="1" />
    <part id="bd_part_1">
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0" /> <pad active="0" />
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0" /> <pad active="0" />
    </part>
    <part id="snare_part_1">
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0" /> <pad active="0" />
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0" /> <pad active="0" />
    </part>
    <part id="charley_part_1">
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0" /> <pad active="0" />
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0" /> <pad active="0" />
    </part>
    <part id="ride_part_1">
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
    </part>
    <part id="bass_part_1">
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
        <pad active="0"/> <pad active="0"/> <pad active="0"/> <pad active="0"/>
    </part>
    <pattern id="pattern_1">
        <channel ref="bd"    part="bd_part_1"/>
        <channel ref="snare" part="snare_part_1"/>
        <channel ref="ch"    part="charley_part_1"/>
        <channel ref="ride"  part="ride_part_1" />
        <channel ref="bass"  part="bass_part_1" />
    </pattern>
    <pattern id="pattern_2">
        <channel ref="bd"    part="bd_part_1"/>
        <channel ref="snare" part="snare_part_1"/>
        <channel ref="ch"    part="charley_part_1"/>
        <channel ref="ride"  part="ride_part_1" />
        <channel ref="bass"  part="bass_part_1" />
    </pattern>
    <song tempo="120">
        <pattern ref="pattern_1"/>
        <pattern ref="pattern_2"/>
    </song>
</slag>
